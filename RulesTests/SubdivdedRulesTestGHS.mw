<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#Not refine

QR:=x-&gt; 
    t1*subs(u=u1,v=u1,x) + 
    t1*subs(u=u1,v=1-u1-u1,x) + 
    t1*subs(u=1-u1-u1,v=u1,x) +
    t2*subs(u=1/3,v=1/3,x);

res:=[u1 = 1/5, t1 = 25/96, t2 = -9/32];
#u1:=cos(arctan(3/4)/3)/3 + 1/3; v1:=-cos(arctan(3/4)/3)/6 + 1/3 + sin(arctan(3/4)/3)*sqrt(3)/6;
#u1:=0.6590276223; v1:=0.2319333686;

testFunc:=testFunc1;
print(&quot;Exact Integral &quot;, evalf(int(testFunc, u=0..1-v, v=0..1))); 
print(&quot;Quadrature &quot;, evalf(subs(res, QR(testFunc)))); evalf(int(testFunc, u=0..1-v, v=0..1)-subs(res, QR(testFunc)));
testFunc:=testFunc2;
print(&quot;Exact Integral &quot;, evalf(int(testFunc, u=0..1-v, v=0..1))); 
