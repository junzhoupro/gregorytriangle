<Group view="code" hide-input="false" labelreference="L1714" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#find the top sub-triangle coords
triangletop := proc( row, col, level, uu, vv )
    description &quot;compute the coors of the sub-triangle&quot;;
    local leftV := row/(2^level)*A - (col-1)/2/(2^level)*A
           + ((col-1)/2)/(2^level)*B
           + (1-(row/(2^level)))*C,

    rightV := (row-1)/(2^level)*A - (col-1)/2/(2^level)*A
           + ((col-1)/2+1)/(2^level)*B
           + (1-(row/(2^level)))*C,

    topV:= (row-1)/(2^level)*A - (col-1)/2/(2^level)*A 
           + ((col-1)/2)/(2^level)*B
           + (1-((row-1)/(2^level)))*C;
    #print(leftV); 
    #print(rightV);
    #print(topV); 
    local node := uu * leftV + vv * rightV + (1 - uu - vv) * topV:
    local node_local := u1 * A + v1 * B + (1-u1-v1) * C:
    local eq, i;
    eq[1] := factor(coeff(node,A,1)) - factor(coeff(node_local,A,1)):
    eq[2] := factor(coeff(node,B,1)) - factor(coeff(node_local,B,1)):
    eq[3] := factor(coeff(node,C,1)) - factor(coeff(node_local,C,1)):
    local sys := [seq(eq[i],i=1..3)]: 
    local res := solve(sys, [u1, v1]);
end proc;

#find the bottom sub-triangle coords
triangleflip := proc( row, col, level, uu, vv )
    description &quot;compute the coors of the sub-triangle&quot;;
    local leftV := (row-1)/(2^level)*A - (col-2)/2/(2^level)*A
           + ((col-2)/2)/(2^level)*B
           + (1-((row-1)/(2^level)))*C,

    rightV := (row-1)/(2^level)*A - (col-2)/2/(2^level)*A
           + ((col-2)/2+1)/(2^level)*B
           + (1-(row/(2^level)))*C,

    topV:= (row-1)/(2^level)*A - (col)/2/(2^level)*A 
           + ((col)/2)/(2^level)*B
           + (1-((row-1)/(2^level)))*C;
    #print(leftV); 
    #print(rightV);
    #print(topV); 
    local node := uu * leftV + vv * rightV + (1 - uu - vv) * topV:
    local node_local := u1 * A + v1 * B + (1-u1-v1) * C:
    local eq, i;
    eq[1] := factor(coeff(node,A,1)) - factor(coeff(node_local,A,1)):
    eq[2] := factor(coeff(node,B,1)) - factor(coeff(node_local,B,1)):
    eq[3] := factor(coeff(node,C,1)) - factor(coeff(node_local,C,1)):
    local sys := [seq(eq[i],i=1..3)]: 
    local res := solve(sys, [u1, v1]);
end proc;</Text-field>
</Input>
